{
  "name": "vscode-fontawesome-gallery",
  "displayName": "Font Awesome Gallery",
  "description": "Font Awesome gallery panel with icon images and names.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:vscode-fontawesome-gallery-sidebar",
    "onCommand:vscode-fontawesome-gallery.askQuestion",
    "onCommand:vscode-fontawesome-gallery.refresh"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscode-fontawesome-gallery-sidebar",
          "title": "Font Awesome Gallery",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "vscode-fontawesome-gallery-sidebar": [
        {
          "type": "webview",
          "id": "vscode-fontawesome-gallery-sidebar",
          "name": "Font Awesome Gallery",
          "icon": "media/icon.svg",
          "contextualTitle": "Font Awesome Gallery"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-fontawesome-gallery.askQuestion",
        "category": "Font Awesome Gallery",
        "title": "Ask Question"
      },
      {
        "command": "vscode-fontawesome-gallery.refresh",
        "category": "Font Awesome Gallery",
        "title": "Refresh"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "rollup -c && webpack",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.20.5",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.0.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "postcss": "^8.2.8",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.35.0",
    "svelte-check": "^1.2.5",
    "svelte-preprocess": "^4.6.9",
    "tailwindcss": "^2.0.3",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "svelte-clipboard": "^0.0.2"
  }
}
